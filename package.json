{
  "name": "kebisheng",
  "version": "0.0.1",
  "description": "使用 React 将 markdown 文件生成 SPA 页面",
  "bin": {
    "bisheng": "./bin/bisheng"
  },
  "files": [
    "lib",
    "bin",
    "router.js",
    "collect.js"
  ],
  "scripts": {
    "build": "babel src --out-dir lib -w",
    "clean": "rimraf lib",
    "prepublish": "npm run build && shx cp src/template.html lib/template.html",
    "postpublish": "npm run clean"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "Firefox ESR",
              "> 1%",
              "ie >= 8",
              "iOS >= 8",
              "Android >= 4"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "publishConfig": { "registry": "https://npm.pkg.github.com/jacob-lcs" },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-lcs/kebisheng.git"
  },
  "keywords": [
    "markdown",
    "spa",
    "website",
    "blog",
    "react"
  ],
  "author": "Jacob<jacob.lcs@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacob-lcs/kebisheng/issues"
  },
  "homepage": "https://github.com/jacob-lcs/kebisheng#readme",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "~0.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "commander": "^4.0.1",
    "css-loader": "^3.4.0",
    "exist.js": "^0.3.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gh-pages": "^2.1.1",
    "history": "^3.3.0",
    "jsonml-to-react-element": "^1.0.0",
    "jsonml.js": "^0.1.0",
    "less": "~3.9.0",
    "less-loader": "^5.0.0",
    "loader-utils": "^1.1.0",
    "mark-twain": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^0.5.1",
    "node-prismjs": "^0.1.0",
    "nprogress-for-antd": "^0.2.0",
    "nunjucks": "^2.5.2",
    "postcss": "^7.0.6",
    "postcss-loader": "~3.0.0",
    "prismjs": "^1.4.1",
    "ramda": "^0.26.0",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.1",
    "react-helmet-async": "^1.0.6",
    "react-router": "^3.0.0",
    "resolve": "^1.1.7",
    "rucksack-css": "~1.0.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.2",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^6.2.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.25.1",
    "webpack-dev-server": "^3.1.10",
    "webpackbar": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "postcss-svgo": "^2.1.6",
    "rimraf": "^3.0.0",
    "shx": "^0.3.2"
  },
  "engines": {
    "node": ">=8.6.0"
  }
}
